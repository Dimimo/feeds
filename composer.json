{
    "name": "willvincent/feeds",
    "description": "Laravel 5 Service Provider for the SimplePie library",
    "license": "MIT",
    "authors": [
        {
            "name": "Will Vincent",
            "email": "tcindie@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0|^9.0",
        "simplepie/simplepie": "1.5.*",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "willvincent\\Feeds\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "willvincent\\Feeds\\FeedsServiceProvider"
            ],
            "aliases": {
                "Feeds": "willvincent\\Feeds\\Facades\\FeedsFacade"
            }
        }
    }
}
