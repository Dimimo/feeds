{
    "name": "dimimo/feeds",
    "description": "Laravel 5 Service Provider for the SimplePie library",
    "license": "MIT",
    "authors": [
        {
            "name": "Will Vincent",
            "email": "tcindie@gmail.com"
        },
        {
            "name": "Dimitri Mostrey",
            "email": "admin@puertoparrot.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "ext-curl": "*",
        "laravel/framework": "^8.75|^9.0",
        "simplepie/simplepie": "1.5.*"
    },
    "autoload": {
        "psr-4": {
            "Dimimo\\Feeds\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/",
            "Dimimo\\Feeds\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dimimo\\Feeds\\FeedsServiceProvider"
            ],
            "aliases": {
                "Feeds": "Dimimo\\Feeds\\Facades\\FeedsFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
